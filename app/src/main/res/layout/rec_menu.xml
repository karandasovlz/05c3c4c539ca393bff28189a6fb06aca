<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fon_eda"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.ht.healthtarget.Recep"
    tools:showIn="@layout/app_bar_main2">

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="320dp"
        android:layout_height="120dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/knopka_1_1"
        app:layout_constraintBottom_toTopOf="@+id/imageButton11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="320dp"
        android:layout_height="120dp"
        android:background="@drawable/knopka_2_2"
        app:layout_constraintBottom_toTopOf="@+id/imageButton12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton10" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="320dp"
        android:layout_height="120dp"
        android:background="@drawable/knopka_3_3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton11" />
</android.support.constraint.ConstraintLayout>